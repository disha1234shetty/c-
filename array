#include <stdio.h>

int main()
{
    int a[5];
    int i,n;
    printf("enter size of array");
    scanf("%d",&n);
    printf("enter arrayelement\n");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("array element are\n");
    for(i=0;i<n;i++)
    {
        printf("%d",a[i]);
    }
}
